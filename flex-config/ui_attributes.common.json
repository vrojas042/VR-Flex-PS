          "other": {
            "color": "darkred",
            "icon": "More"
          }
        },
        "queuesStatsColumns": {
          "assignedTasksColumn": true,
          "wrappingTasksColumn": true
        }
      },
      "sip_support": {
        "enabled": false
      }
    }
  }
